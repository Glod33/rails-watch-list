<%= @list.name %>
<% @list.bookmarks.each do |b| %>
  <%= b.movie.title %>
  <%= b.movie.rating %>
  <%= b.movie.overview %>
  <%= b.comment %>
<% end %>
<%= link_to 'Create a bookmark', new_list_bookmark_path(@list), class: "btn btn-primary" %>
<%= link_to "Delete this list",
  list_path(@list),
  data: {turbo_method: :delete, turbo_confirm: "Are you sure?"},
  class: "btn btn-warning"
%>
